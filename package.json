{
  "name": "100MenWhoCare",
  "version": "1.0.0",
  "description": "100 who care web app",
  "scripts": {
    "prestart": "babel-node tools/devStartMessage.js",
    "start": "npm-run-all --parallel open:srcExpress lint:watch test:watch",
    "open:srcExpress": "babel-node tools/srcExpress.js",
    "lint": "node_modules/.bin/esw webpack.config.* src tools",
    "lint:watch": "npm run lint -- --watch",
    "test": "mocha --reporter spec tools/testSetup.js src/**/*.test.js src/components/**/*.test.js",
    "test:watch": "npm run test -- --watch",
    "remove-dist": "rimraf dist",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "build:html": "babel-node tools/buildHtml.js",
    "prebuild": "npm-run-all clean-dist test lint build:html",
    "build:prodWebpack": "babel-node tools/build.js",
    "build": "npm run build:prodWebpack",
    "postbuild": "babel-node tools/herokuExpress.js",
    "predeploy:heroku": "npm-run-all clean-dist build:html",
    "deploy": "npm run build:prodWebpack",
    "postdeploy:heroku": "babel-node tools/herokuExpress.js",
    "heroku:deploy": "npm-run-all predeploy:heroku deploy postdeploy:heroku"
  },
  "author": "Amir Kaudinov",
  "license": "MIT",
